ntainer:""};this.element=b,this.options=a.extend({},d,c),this.init()}d.prototype={init:function(){var c,d,e,f,g=this,h=a(g.element);c=h.offset().top,d=h.height(),""!==g.options.container&&(e=a(g.options.container).offset().top,f=a(g.options.container).height()),a(b).on("resize.sticky",function(b){""!==g.options.container&&(e=a(g.options.container).offset().top,f=a(g.options.container).height())}),a(b).on("scroll.sticky load.sticky resize.sticky",function(i){if(a(b).width()>=1056){var j=a(b).scrollTop();if(j>c-g.options.marginTop)if(""!==g.options.container)if(j>e+f-d-g.options.marginTop)if(h.addClass("grantland-fixed-bottom"),h.parents(g.options.container).length)h.css("bottom",0);else{var k=h.offsetParent().height()+h.offsetParent().offset().top-(f+e)-6;h.css("bottom",k)}else h.addClass("grantland-fixed").removeClass("grantland-fixed-bottom").css("bottom","auto").css("width",h.parent().width()).css("top",g.options.marginTop);else h.addClass("grantland-fixed").css("width",h.parent().width()).css("top",g.options.marginTop);else h.removeClass("grantland-fixed").attr("style","")}else h.removeClass("grantland-fixed").attr("style","")})}},a.extend(a.fn,{sticky:function(b){return this.each(function(){a.data(this,"grantland_sticky")||a.data(this,"grantland_sticky",new d(this,b))})}}),a(b).load(function(){a("section.top-stories[data-stick-it]").sticky({container:".layout-sidebar"}),a(".social-share-list[data-stick-it]").sticky({container:".article-body",marginTop:16})})}(jQuery,this),jQuery(function(a){a(".article-body").fitVids({customSelector:"iframe[src*='vine.co'], iframe[src*='instagram.com']"}),a(".blog-body").fitVids({customSelector:"iframe[src*='vine.co'], iframe[src*='instagram.com']"});var b=a(".photo-credit-wrapper"),c=a(".expand-size-full");b.length&&b.each(function(){var b=a(this),d=b.find("> a.thickbox"),e=b.find("> img.alignnone.size-full"),f=b.find(".aligncenter, .alignleft, .alignright");if(c.length&&e.length&&!d.length,c.length&&d.length,f.length){var g=/(^|\s)(align(center|left|right))(\s|$)/,h=((f.attr("class")||"").match(g)||[])[2];h&&(f.removeClass(h),b.addClass(h))}}),a(".btn_newsletter_signup").length&&(a(".section.newsletter, .btn_newsletter_signup").on("click",function(b){b.preventDefault(),a("#newsletter_modal").modal({fadeDuration:200,zIndex:100})}),a("#newsletter_modal").on(a.modal.BEFORE_OPEN,function(b,c){var d=a(c.elm).find("form"),e=a(c.elm).find("#newsletter-signup-response"),f=a(c.elm).find("#newsletter-main-copy"),g=a(c.elm).find(".newsletter-success");d.trigger("reset"),d.show(),e.html(""),g.hide().html(""),f.show()}),a("#fanbridge_signup").submit(function(b,c){var d=a(b.currentTarget),e=a("#newsletter-signup-response"),f=a("#newsletter-main-copy"),g=a(".newsletter-success");return e.hide(),g.length||(g=f.after(a('<p class="newsletter-success"></p>')).parent().find(".newsletter-success")),a.ajax({type:"POST",url:d.attr("action")+"&response=json",data:d.serialize(),dataType:"jsonp",success:function(a){"ok"==a.status?d.slideUp("slow",function(){f.slideUp("fast",function(){e.hide(),g.html("Thanks for submitting!<br>You should receive a confirmation in your inbox shortly.").fadeIn("slow")})}):"error"==a.status&&e.show().addClass("error").html(a.data.fields.email)},error:function(a){e.addClass("error").text("Oops! There was an issue. Try again in a moment?")}}),!1}))});;
/*
 * Thickbox 3.1 - One Box To Rule Them All.
